{
  "name": "huayra-stopmotion",
  "version": "0.6.1",
  "description": "el StopMotion de Huayra",
  "main": "index.js",
  "scripts": {
    "test": "nodewebkit src",
    "start": "nw src",
    "dist:win": "build --tasks win-x86,win-x64 --mirror https://dl.nwjs.io/ .",
    "dist:osx": "build --tasks mac-x64 --mirror https://dl.nwjs.io/ .",
    "dist:linux86": "build --tasks linux-x86 --mirror https://dl.nwjs.io/ .",
    "dist": "build --tasks linux-x64 --mirror https://dl.nwjs.io/ .",
    "release": "npm version patch && git push origin $(git rev-parse --abbrev-ref HEAD) && git push origin --tags",
    "nwjs-start": "run --x86 --mirror https://dl.nwjs.io/ ."
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:hugoruscitti/stopmotion.git"
  },
  "author": "",
  "license": "BSD-2-Clause",
  "dependencies": {
      "nwjs-hack": "iglosiggio/nwjs-hack",
      "express": "^4.18.2",
      "socket.io": "^4.6.1",
      "jsonfile": "~6.1.0",
      "gaze": "^1.1.3",
      "q": "~1.5.1",
      "async": "^3.2.4",
      "bytes": "^3.1.2",
      "qs": "^6.11.0",
      "active-x-obfuscator": "^0.0.2",
      "uglify-js": "^3.17.4"
  },
  "devDependencies": {
    "configstore": "^6.0.0",
    "dot-prop": "^7.2.0",
    "got": "^12.5.3",
    "latest-version": "^7.0.0",
    "meow": "^11.0.0",
    "nw": "^0.73.0",
    "nwjs": "^1.4.4",
    "nwjs-builder-phoenix": "^1.15.0",
    "nwjs-packager": "^1.5.1",
    "package-json": "^8.1.0",
    "pretty-bytes": "^6.1.0",
    "trim-newlines": "^4.0.2",
    "update-notifier": "^6.0.2"
  },
  "build": {
    "nwVersion": "0.73.0"
  }
}
